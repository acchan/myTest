	<!-- Global resources used by this theme -->
	<!--
	<resource>
		<content-type>text/css</content-type>
		<href>/.ibmxspres/global/theme/stylesheet.css</href>
	</resource>
	-->

	<!-- Property values -->
	<!--
	<control>
		<name>[Control Name]</name>
		<property>
			<name>[property Name]</name>
			<value>[property Value]</value>
		</property>
	</control>
	-->
	<resource>
		<content-type>text/css</content-type>
		<href>blue.css</href>
	</resource>
	<control>
		<name>Text.Label</name>
		<property>
			<name>styleClass</name>
			<value>textBase</value>
		</property>
	</control>
	<control>
		<name>Header</name>
		<property>
			<name>styleClass</name>
			<value>textHeaderBlue</value>
		</property>
	</control>
	<control>
		<name>Button.Cancel</name>
		<property>
			<name>styleClass</name>
			<value>buttonCreateBlue</value>
		</property>
	</control>
	<control>
		<name>SearchCriteria</name>
		<property>
			<name>styleClass</name>
			<value>#{javascript:(sessionScope.nameToSearch != null) ?
'textOk' : 'textError'}</value>
		</property>
	</control>
	<control>
		<name>ViewRoot</name>
		<property>
			<name>styleClass</name>
			<value>bodyBlue tundra</value>
		</property>
	</control>
 	<control>
		<name>TabBar</name>
		<property>
			<name>styleClass</name>
			<value>tabBarBlue</value>
		</property>
	</control>
</theme>
